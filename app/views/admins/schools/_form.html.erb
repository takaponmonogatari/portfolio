<div class="container mt-5">
		<h3>新規投稿</h3>
 <%= form_with model:school,url: url, local:true do |f| %>
   <div class="form-group">
     <%= f.label :name, "学校名" %>
     <%= f.text_field :name, class:"form-control" %>
   </div>
   <div class="form-group">
     <%= f.label :name_kana, "学校名カナ" %>
     <%= f.text_field :name_kana, class:"form-control" %>
   </div>
   <div class="form-group">
     <%= f.label :prefecture_id, "都道府県" %>
     <%= f.collection_select :prefecture_id, Prefecture.all, :id, :name, class:"form-control" %>
   </div>
   <div class="form-group">
     <%= f.label :division_id, "学校区分" %>
     <%= f.collection_select :division_id, Division.all, :id, :name, class:"form-control" %>
   </div>
   <div class="form-group">
     <%= f.label :postal_code, "郵便番号" %>
     <%= f.text_field :postal_code, class:"form-control" %>
   </div>
   <div class="form-group">
     <%= f.label :address, "住所" %>
     <%= f.text_field :address, class:"form-control" %>
   </div>
   <div class="form-group">
     <%= f.label :url, "学校HP" %>
     <%= f.text_field :url, class:"form-control" %>
   </div>
   <div class="form-group">
     <%= f.label :club, "部活" %>
     <%= f.text_field :club, class:"form-control" %>
   </div>

   <%= f.submit submit %>
   <% end %>
 </div>